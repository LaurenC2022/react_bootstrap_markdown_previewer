{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommend",
    "plugin:jsx-a11y/recommend",
    "prettier"
  ],
  "rules": { "react/prop-types": 0, "react/jsx-in-jsx-scope": 0 },
  "plugins": ["react", "import", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": { "version": "detect" }
  },
  "import/resolver": {
    "node": {
      "extensions": [".js", ".jsx"]
    }
  }
}
